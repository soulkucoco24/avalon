[request]
default_page = "index.html"

[server]
host = "shikii.cc"
port = 80
max_request = 1700
worker_num = 1
webroot = 'shikii.cc'
;document_root = "/public"
;process_rename = 1
keepalive = 1
;gzip_open = 1
;user = www-data
expire_open = 1

[session]
cookie_life = 1800
session_life = 1800

[access]
deny_dir = "libs,class,templates"
static_dir = "/public,/public/static,/apps/templates/"
static_ext = "js,jpg,gif,png,css,html,ico"
dynamic_ext = "php"
post_maxsize = 200000

[apps]
charset = "utf-8"
;auto_reload = 1
apps_path = "/apps"
;是否处理静态文件
do_static = on
